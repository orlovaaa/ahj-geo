{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },

  "rules": {
    "indent": [2, "tab"],
    "no-tabs": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}